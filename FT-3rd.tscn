script/source = "extends RigidBody3D

func _ready() -> void: # Initalises once when starting
	pass
	
func _process(delta: float) -> void: # Updates every frame
	var Forward = Input.get_action_strength(\"MoveForward\")
	apply_central_force(Forward * Vector3.FORWARD * 1200.0 * delta)
	var Back = Input.get_action_strength(\"MoveBackward\")
	apply_central_force(Back * Vector3.BACK * 1200.0 * delta)
	var Right = Input.get_action_strength(\"MoveRight\")
	apply_central_force(Right * Vector3.RIGHT * 1200.0 * delta)
	var Left = Input.get_action_strength(\"MoveLeft\")
	apply_central_force(Left * Vector3.LEFT * 1200.0 * delta)
	pass
		
# Friction
"

